                android:name="com.karimo.sumit_final.MainForm"
                android:label="@string/app_name" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.karimo.sumit_final.SummaryScreen"
            android:label="@string/title_activity_summary_screen" >
        </activity>
    </application>
<uses-permission android:name="android.permission.INTERNET"/>

</manifest>
